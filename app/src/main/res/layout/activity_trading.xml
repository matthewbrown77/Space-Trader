<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/furs_buy"
        android:layout_width="80sp"
        android:layout_height="40sp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/pixeled"
        android:text="Buy"
        android:textSize="7sp"
        app:layout_constraintEnd_toStartOf="@+id/furs_sell"
        app:layout_constraintTop_toBottomOf="@+id/water_buy"
        android:onClick="onClickBuyFurs"/>

    <Button
        android:id="@+id/food_buy"
        android:layout_width="80sp"
        android:layout_height="40sp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/pixeled"
        android:text="Buy"
        android:textSize="7sp"
        app:layout_constraintEnd_toStartOf="@+id/food_sell"
        app:layout_constraintTop_toBottomOf="@+id/furs_buy"
        android:onClick="onClickBuyFood"/>

    <View
