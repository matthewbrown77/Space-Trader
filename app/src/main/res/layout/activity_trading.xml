        android:fontFamily="@font/pixeled"
        android:text="Buy"
        android:textSize="7sp"
        app:layout_constraintEnd_toStartOf="@+id/robots_sell"
        app:layout_constraintTop_toBottomOf="@+id/narcotics_buy"
        android:onClick="onClickBuyRobots"/>

    <Button
        android:id="@+id/narcotics_buy"
        android:layout_width="80sp"
        android:layout_height="40sp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/pixeled"
        android:text="Buy"
