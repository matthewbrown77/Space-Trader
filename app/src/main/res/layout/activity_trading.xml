        android:id="@+id/water_sell"
        android:layout_width="80sp"
        android:layout_height="40sp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="33dp"
        android:fontFamily="@font/pixeled"
        android:text="Sell"
        android:textSize="7sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trading_cargo"
        android:onClick="onClickSellWater"/>

    <Button
        android:id="@+id/robots_sell"
        android:layout_width="80sp"
        android:layout_height="40sp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="33dp"
        android:fontFamily="@font/pixeled"
        android:text="Sell"
        android:textSize="7sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/narcotics_sell"
        android:onClick="onClickSellRobots"/>

    <Button
        android:id="@+id/narcotics_sell"
        android:layout_width="80sp"
        android:layout_height="40sp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="33dp"
        android:fontFamily="@font/pixeled"
        android:text="Sell"
        android:textSize="7sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/machines_sell"
