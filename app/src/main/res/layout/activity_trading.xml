    <TextView
        android:id="@+id/ore_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="17dp"
        android:fontFamily="@font/pixeled"
        android:text="Ore - $350"
        android:textSize="9sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/food_title" />

    <TextView
        android:id="@+id/games_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="17dp"
        android:fontFamily="@font/pixeled"
        android:text="Game - $250"
        android:textSize="9sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ore_title" />

    <TextView
        android:id="@+id/firearms_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="17dp"
        android:fontFamily="@font/pixeled"
        android:text="Firearms - $1250"
        android:textSize="9sp"
        app:layout_constraintStart_toStartOf="parent"
